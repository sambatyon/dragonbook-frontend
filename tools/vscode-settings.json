{
  "java.project.sourcePaths": [
    "java"
  ],
  "go.goroot": "${workspaceFolder}/bazel-dragonbook/external/go_sdk",
  "go.toolsEnvVars": {
    "GOPACKAGESDRIVER": "${workspaceFolder}/tools/gopackagesdriver.sh"
  },
  "go.enableCodeLens": {
    "references": false,
    "runtest": false
  },
  "gopls": {
    "build.directoryFilters": [
      "-bazel-bin",
      "-bazel-out",
      "-bazel-testlogs",
      "-bazel-dragonbook",
    ],
    "formatting.gofumpt": true,
    "formatting.local": "dragonbook",
    "ui.completion.usePlaceholders": true,
    "ui.semanticTokens": true,
    "ui.codelenses": {
      "gc_details": false,
      "regenerate_cgo": false,
      "generate": false,
      "test": false,
      "tidy": false,
      "upgrade_dependency": false,
      "vendor": false
    },
  },
  "go.languageServerFlags": [
   // "-rpc.trace"
  ],
  "go.useLanguageServer": true,
  "go.buildOnSave": "off",
  "go.lintOnSave": "off",
  "go.vetOnSave": "off",
  "files.associations": {
    "memory": "cpp",
    "__bit_reference": "cpp",
    "__bits": "cpp",
